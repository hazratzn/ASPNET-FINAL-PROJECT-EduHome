}

<div class="container mt-3">
    <div class="d-flex justify-content-between">
        <h2>Welcome Banner</h2>
    </div>
    <div class="table-responsive pt-3">
        <table class="table table-dark">
            <thead>
                <tr>
                    <th>
                        #
                    </th>
                    <th>
                        Link
                    </th>
                    <th>
                        Video 
                    </th>
                   

                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>
                        1
                    </td>
                    <td>
                        @Model.Link
