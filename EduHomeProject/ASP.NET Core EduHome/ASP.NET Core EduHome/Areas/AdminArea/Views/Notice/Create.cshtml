
@model Notice
@{
    ViewData["Title"] = "Create";
}

<div class="container">
    <div class="col-md-6 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">

                <form class="forms-sample" method="post" asp-action="Create" enctype="multipart/form-data">
                    <div class="form-group">
                        <label asp-for="Date"></label>
                        <input class="form-control" asp-for="Date">
                        <span class="text-danger" asp-validation-for="Date"></span>

                        

                        <label asp-for="NoticeBoard"></label>
                        <input class="form-control" asp-for="NoticeBoard">
                        <span class="text-danger" asp-validation-for="NoticeBoard"></span>


                    </div>
                    <button type="submit" class="btn btn-primary me-2">Create</button>
                    <button class="btn btn-light">
                        <a asp-action="Index">Cancel</a>
                    </button>
                </form>
            </div>
        </div>
    </div>
</div>
